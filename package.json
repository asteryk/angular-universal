{
  "name": "angular-universal",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "webpack",
    "build:aot": "webpack --env.aot --env.client & webpack --env.aot --env.server",
    "build:prod": "webpack --env.aot --env.client -p & webpack --env.aot --env.server",
    "prebuild": "npm run clean",
    "prebuild:aot": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist && rimraf build",
    "server": "nodemon dist/server.js",
    "watch": "webpack --watch",
    "test": "nodemon build/server.js",
    "build-dev": "npm run clean && webpack --config ./config/webpack.dev.client.js && webpack --config ./config/webpack.dev.server.js",
    "build-prod": "npm run clean && webpack --config ./config/webpack.prod.client.js && webpack --config ./config/webpack.prod.server.js"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.0.0-rc.3",
    "@angular/compiler": "^4.0.0-rc.3",
    "@angular/core": "^4.0.0-rc.3",
    "@angular/http": "^4.0.0-rc.3",
    "@angular/platform-browser": "^4.0.0-rc.3",
    "@angular/platform-browser-dynamic": "^4.0.0-rc.3",
    "@angular/platform-server": "^4.0.0-rc.3",
    "@angular/router": "^4.0.0-rc.3",
    "express": "^4.15.2",
    "rxjs": "^5.2.0",
    "xhr2": "^0.1.4",
    "zone.js": "^0.8.1",
    "@ntesmail/shark-css": "^1.2.8"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0-rc.3",
    "@ngtools/webpack": "^1.2.12",
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.8",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-merge": "^4.0.0",
    "node-sass": "^4.5.0",
    "html-webpack-plugin": "^2.28.0",
    "to-string-loader": "^1.1.5",
    "css-loader": "^0.26.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.0"
  }
}
